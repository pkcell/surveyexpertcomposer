{
  "name": "greenbit/surveyexpert",
  "description": "A Laravel package for managing surveys",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "GreenBit\\": "src/"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "GreenBit\\SurveyServiceProvider"
      ],
      "aliases": {
        "SurveyProvider": "GreenBit\\Facades\\SurveyProvider",
        "SurveyBuilder": "GreenBit\\Facades\\SurveyBuilder"
      }
    }
  }
}
